"use strict";var colsch=NGL.ColormakerRegistry.addScheme(function(params){this.atomColor=function(atom){if(atom.atomType.element=="B")return 0x808080;else if(atom.atomType.element=="P")return 0xFFFF00;else if(atom.atomType.element=="I")return 0x0000FF;else if(atom.atomType.element=="S")return 0xFF0000;else if(atom.atomType.element=="O")return 0xFF00FF;else if(atom.atomType.element=="N")return 0x008000;else if(atom.atomType.element=="C")return 0x00FFFF;};});function dsrA(component){if(component.type!=="structure")return false;component.addRepresentation("spacefill",{sele:"all",radius:1.6,color:colsch,opacity:0.5});}function dsrB(component){if(component.type!=="structure")return;component.addRepresentation("spacefill",{sele:"all",radius:0.8,color:colsch,opacity:0.5});}function dsrAs(component){if(component.type!=="structure")return;component.addRepresentation("spacefill",{sele:".C .N .O .S .Y .P .I",radius:1.6,color:colsch,opacity:0.5});}function dsrBs(component){if(component.type!=="structure")return;component.addRepresentation("spacefill",{sele:".C .N .O .S .Y .P .B .I",radius:0.8,color:colsch,opacity:0.5});}function dsrC(component){if(component.type!=="structure")return;component.addRepresentation("ball+stick",{sele:"not (_H or _HC)",scale:2.5});}function ingl(idx, oi){for(var i=0;i<4;i++){stage[idx].getComponentsByName().list[i].setVisibility(false);}oi.forEach(function(ox){stage[idx].getComponentsByName().list[ox].setVisibility(true);});}var resultInfo=[];var viewport=[];var stage=[];var hs='hide';$(document).ready(function(){$("[data-record]").on("click",function(){var id=$(this).attr('data-record');if(stage[id]===undefined){$("[data-display='"+id+"']").show();stage[id]=new NGL.Stage(viewport[id],{backgroundColor:"white",tooltip: false});stage[id].loadFile(resultInfo["Ua"+id]).then(dsrA).then(function(){stage[id].loadFile(resultInfo["Ua"+id]).then(dsrAs).then(function(){stage[id].loadFile(resultInfo["Ub"+id]).then(dsrB).then(function(){stage[id].loadFile(resultInfo["Ub"+id]).then(dsrBs).then(function(){stage[id].loadFile(resultInfo["Uc"+id]).then(dsrC).then(function(){stage[id].autoView();});});});});});}else{$("[data-display='" + id + "']").toggle();}if($("[data-record='"+id+"']")[0].innerText==="+"){$("[data-record='"+id+"']")[0].innerText="-";}else{$("[data-record='"+id+"']")[0].innerText="+";}});var tio=0;$("button[data-ta]").on("mouseover",function(event){$("button[data-ta]").popover('hide');var ts=event.target;tio=setTimeout(function(){$(ts).popover('show');}, 300);});$("button[data-ta]").on("mouseout",function(){clearTimeout(tio);})});function fs(o){stage[o].toggleFullscreen();}
